<?xml version="1.0" encoding="UTF-8"?>
<wdkModel>
    <modelName displayName="TemplateDB" version="1.0"/>

    <!-- ******************* -->
    <!-- model introduction  -->
    <!-- ******************* -->
    <introduction><![CDATA[
        Welcome to Strategies WDK Template Site.  It uses <a href="https://www.cbil.upenn.edu/svn/gus/WDKTemplateSite/trunk/Model/data/">sample data</a>, a <a href="https://www.cbil.upenn.edu/svn/gus/WDKTemplateSite/trunk/Model/lib/wdk/">simple model</a>, and <a href="https://www.cbil.upenn.edu/svn/gus/WDKTemplateSite/trunk/Site/">simple view customization</a> to illustrate the core functionality of the WDK. It is can also be used as a template for installing a real WDK site. Please see the <a href="http://code.google.com/p/strategies-wdk/">project home</a> for more details.]]>
        </introduction>
    
    <!-- import the pieces -->
    <import file="templateModel/records/rnaRecord.xml" />
    <import file="templateModel/records/rnaRecordQueries.xml" />
    <import file="templateModel/questions/rnaQuestions.xml" />
    <import file="templateModel/questions/rnaIdQueries.xml" />

    <import file="templateModel/records/estRecord.xml" />
    <import file="templateModel/records/estRecordQueries.xml" />
    <import file="templateModel/questions/estQuestions.xml" />
    <import file="templateModel/questions/estIdQueries.xml" />

    <import file="templateModel/records/xmlRecords.xml" />
    <import file="templateModel/questions/xmlQuestions.xml" />


    <import file="templateModel/questions/nestedQuestions.xml" />
    <import file="templateModel/questions/params.xml" />

    <!-- Categories for driving menus -->
    <import file="templateModel/questions/categories.xml"/>

</wdkModel>
